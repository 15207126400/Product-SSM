	<!-- 当前表查询总条数 -->
	<select id="queryTotals" parameterType="java.util.Map" resultType="int">
		select count(1) from yw_withdraw
		<include refid="whereSql"/>
	</select>
	
	<!-- 当前表更新 -->
	<update id="update" parameterType="java.util.Map">
	    update yw_withdraw
	     <trim prefix="set" suffixOverrides=",">
            <if test="withdraw_price != null and withdraw_price != '' ">
	           withdraw_price = #{withdraw_price},
	        </if>
            <if test="withdraw_type != null and withdraw_type != '' ">
	           withdraw_type = #{withdraw_type},
	        </if>
