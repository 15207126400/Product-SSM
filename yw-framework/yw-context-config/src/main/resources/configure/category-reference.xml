        <generate template="/counter/livebosconfig.properties" />
        <generate template="/counter/t2sdk-config.xml" />
        <generate template="/counter/t2.properties" />
        <generate template="/counter/ets.properties" />
    </catalog>
    
    <!-- 邮件配置 -->
    <catalog id="mail" name="邮件配置" description="邮件地址配置" config="/mail/config.xml">
        <generate template="/mail/mail.properties" />
    </catalog>
    
    <!-- 存储配置 -->
    <catalog id="store" name="存储配置" description="存储及发布配置" config="/store/config.xml">
        <generate template="/store/upload.properties" />
        <generate template="/store/upload-tomcat-context.xml" />
    </catalog>
    <!-- 语音验证码 -->
    <catalog id="ccprest" name="语音验证码配置" description="语音验证码配置" config="/ccprest/config.xml">
