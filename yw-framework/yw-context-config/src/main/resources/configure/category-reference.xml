    <!-- 网厅&小贷通配置
    <catalog id="wt" name="网厅/小贷通配置" description="网厅/小贷通配置" config="/wt/config.xml">
        <generate template="/ /wt-common.properties" />
        <generate template="/wt/wt-biz.properties" />
    </catalog>
    -->
    
