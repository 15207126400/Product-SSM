    <!-- 配置功能列表， id 必须唯一的不能重复  config 为配置规则文件，  generate为配置内容模板， 通过占位符替换  -->
    
    <!-- 数据库配置
    <catalog id="db" name="数据库配置" description="数据库地址配置" config="/db/config.xml">
        <generate template="/db/jdbc.properties" />
    </catalog> -->
    
    <!-- 域名配置 -->
    <catalog id="conn" name="域名配置" description="前中后台域名配置" config="/conn/config.xml">
        <generate template="/conn/connection.properties" />
    </catalog>
    
    <!-- 券商个性化 -->
    <catalog id="sec" name="券商个性化配置" description="券商个性化显示配置" config="/sec/config.xml">
        <generate template="/sec/securities.properties" />
        <generate template="/sec/systemGreeting.properties" />
    </catalog>
    
    <!-- 功能显示开发 -->
    <catalog id="func" name="功能开关" description="券商显示功能开关" config="/func/config.xml">
        <generate template="/func/func.properties" />
    </catalog>
    
    <!-- redis配置 
    <catalog id="redis" name="Redis配置" description="Redis配置" config="/redis/config.xml">
        <generate template="/redis/jedis.xml" />
    </catalog>
    -->
    <!-- CA配置 -->
    <catalog id="ca" name="CA配置" description="中登、天威、同花顺等CA配置" config="/ca/config.xml">
        <generate template="/ca/ca.properties" />
        <generate template="/ca/netsignagent.properties"/>
        <generate template="/ca/systemNos.properties"/>
         <generate template="/ca/BJCAROOT/SVSClient.properties"/>
    </catalog>
    
     <!-- 公安配置 -->
